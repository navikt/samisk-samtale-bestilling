{
    "name": "samisk-samtale-bestilling-server",
    "version": "2.0.14",
    "private": true,
    "scripts": {
        "build": "tsc",
        "start": "node ./dist/server/src/server.js",
        "dev": "npm run build && concurrently \"tsc -w\" \"nodemon -w ./dist/server ./dist/server/src/server.js\""
    },
    "dependencies": {
        "compression": "1.8.1",
        "csp-header": "6.1.0",
        "dotenv": "17.2.1",
        "express": "5.1.0",
        "lru-cache": "11.1.0",
        "node-cache": "5.1.2",
        "react": "npm:@preact/compat",
        "react-dom": "npm:@preact/compat"
    },
    "devDependencies": {
        "@types/compression": "1.8.1",
        "@types/express": "5.0.3",
        "@types/node": "24.3.0",
        "concurrently": "9.2.0",
        "nodemon": "3.1.10"
    },
    "nodemonConfig": {
        "delay": 500
    }
}
