{
    "name": "samisk-samtale-bestilling",
    "version": "2.0.14",
    "private": true,
    "type": "module",
    "packageManager": "pnpm@10.29.3",
    "scripts": {
        "preinstall": "only-allow pnpm",
        "build": "rm -fr dist && pnpm run --sequential '/^build:.*/' ",
        "build:client": "vite build --outDir ./server/dist/client",
        "build:ssr": "vite build --outDir ./server/src/_ssr-dist --ssr src/main-server.tsx",
        "build:server": "pnpm run --filter ./server build",
        "analyze-bundle": "ANALYZE=true pnpm run build:client",
        "start": "pnpm run --filter ./server start",
        "lint": "eslint",
        "start-local": "cp .env.local-prod .env && pnpm run --filter ./server start",
        "decorator-local": "docker compose up",
        "dev-prepare": "node preact-compat-symlinks.js && cp .env.development .env",
        "dev": "pnpm run dev-prepare && pnpm run --filter ./server dev"
    },
    "dependencies": {
        "@grafana/faro-web-sdk": "2.2.0",
        "@navikt/ds-css": "7.40.0",
        "@navikt/ds-react": "7.40.0",
        "@navikt/nav-dekoratoren-moduler": "3.6.1",
        "preact": "10.28.2",
        "@preact/compat": "18.3.1",
        "body-parser": "2.2.2",
        "classnames": "2.5.1",
        "html-react-parser": "5.2.12",
        "jsdom": "27.4.0",
        "node-cache": "5.1.2",
        "preact-render-to-string": "6.6.5",
        "react-router": "7.13.0",
        "vite": "7.3.1"
    },
    "devDependencies": {
        "@babel/preset-react": "7.28.5",
        "@preact/preset-vite": "2.10.2",
        "@types/node": "25.0.10",
        "@types/react": "19.2.9",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/eslint-plugin": "8.53.1",
        "@typescript-eslint/parser": "8.53.1",
        "eslint": "9.39.2",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-react": "7.37.5",
        "globals": "16.5.0",
        "only-allow": "1.2.2",
        "prettier": "3.8.1",
        "rollup-plugin-visualizer": "6.0.5",
        "typescript": "5.9.3"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild",
            "protobufjs"
        ]
    }
}
