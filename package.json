{
    "name": "samisk-samtale-bestilling",
    "version": "2.0.14",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "rm -fr dist && npm-run-all build:*",
        "build:client": "vite build --outDir ./server/dist/client",
        "build:ssr": "vite build --outDir ./server/src/_ssr-dist --ssr src/main-server.tsx",
        "build:server": "npm run build --workspace server",
        "analyze-bundle": "ANALYZE=true npm run build:client",
        "start": "npm run start --workspace server",
        "lint": "eslint",
        "start-local": "cp .env.local-prod .env && npm run start --workspace server",
        "decorator-local": "docker compose up",
        "dev-prepare": "node preact-compat-symlinks.js && cp .env.development .env",
        "dev": "npm run dev-prepare && npm run dev --workspace server"
    },
    "workspaces": [
        "server"
    ],
    "dependencies": {
        "@grafana/faro-web-sdk": "2.1.0",
        "@navikt/ds-css": "7.38.0",
        "@navikt/ds-react": "7.38.0",
        "@navikt/nav-dekoratoren-moduler": "3.6.1",
        "@preact/compat": "18.3.1",
        "body-parser": "2.2.2",
        "classnames": "2.5.1",
        "html-react-parser": "5.2.11",
        "jsdom": "27.3.0",
        "node-cache": "5.1.2",
        "preact-render-to-string": "6.6.4",
        "react-router": "7.12.0",
        "vite": "7.3.0"
    },
    "devDependencies": {
        "@babel/preset-react": "7.28.5",
        "@preact/preset-vite": "2.10.2",
        "@types/node": "25.0.3",
        "@types/react": "19.2.8",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "eslint": "9.39.2",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-react": "7.37.5",
        "globals": "16.5.0",
        "npm-run-all": "4.1.5",
        "prettier": "3.7.4",
        "rollup-plugin-visualizer": "6.0.5",
        "typescript": "5.9.3"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
