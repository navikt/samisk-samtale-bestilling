{
    "name": "samisk-samtale-bestilling",
    "version": "1.3.2",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "rm -fr dist && npm-run-all build:*",
        "build:client": "vite build --outDir ./server/dist/client",
        "build:ssr": "vite build --outDir ./server/src/_ssr-dist --ssr src/main-server.tsx",
        "build:server": "npm run build --workspace server",
        "analyze-bundle": "ANALYZE=true npm run build:client",
        "start": "npm run start --workspace server",
        "lint": "eslint",
        "start-local": "cp .env.local-prod .env && npm run start --workspace server",
        "decorator-local": "docker compose up",
        "dev-prepare": "node preact-compat-symlinks.js && cp .env.development ./server/dist/.env",
        "dev": "npm run dev-prepare && npm run dev --workspace server"
    },
    "workspaces": [
        "server"
    ],
    "dependencies": {
        "@navikt/ds-css": "5.6.4",
        "@navikt/ds-react": "5.6.4",
        "@navikt/nav-dekoratoren-moduler": "2.1.3",
        "@preact/compat": "17.1.2",
        "amplitude-js": "8.21.9",
        "body-parser": "1.20.2",
        "classnames": "2.3.2",
        "html-react-parser": "4.2.2",
        "jsdom": "22.1.0",
        "node-cache": "5.1.2",
        "preact-render-to-string": "6.2.2",
        "react-router-dom": "6.16.0"
    },
    "devDependencies": {
        "@babel/preset-react": "7.22.15",
        "@preact/preset-vite": "2.5.0",
        "@types/node": "20.8.4",
        "@types/react-dom": "18.2.12",
        "@typescript-eslint/eslint-plugin": "6.7.5",
        "@typescript-eslint/parser": "6.7.5",
        "eslint": "8.51.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-react": "7.33.2",
        "husky": "8.0.3",
        "npm-run-all": "4.1.5",
        "prettier": "3.0.3",
        "rollup-plugin-visualizer": "5.9.2",
        "typescript": "5.2.2",
        "vite": "4.4.11"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
